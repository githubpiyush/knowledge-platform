{
  "$id": "boxset-schema.json",
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "BoxSet",
  "type": "object",
  "required": [
    "title",
    "bookCount",
    "summary",
    "cost",
    "author",
    "weight",
    "year",
    "bookList",
    "status"
  ],
  "properties": {
    "title": {
      "type": "string"
    },
    "bookCount": {
      "type": "number"
    },
    "summary": {
      "type": "string",
      "description": "External Property"
    },
    "bookList": {
      "type": "array",
      "items": {
        "type": "string"
      }
    },
    "cost": {
      "type": "number"
    },
    "author": {
      "type": "string"
    },
    "weight": {
      "type": "number"
    },
    "year": {
      "type": "string"
    },
    "status": {
      "type": "string",
      "enum": [
        "sold-out",
        "in-stock",
        "under-review"
      ]
    }
  }
}